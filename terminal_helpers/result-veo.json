{
  "Gemini": {
    "VEO": {
      "provider": "Gemini",
      "service": "VEO",
      "possible_parameters": {
        "config_aspectRatio": [
          "16:9",
          "9:16"
        ],
        "config_durationSeconds": [
          "4",
          "6",
          "8"
        ],
        "config_personGeneration": [
          "allow_all",
          "allow_adult",
          "dont_allow"
        ],
        "resolution": [
          "720p",
          "1080p",
          "4K"
        ],
        "model": [
          "veo-3.0-generate",
          "veo-3.0-fast-generate",
          "veo-3.1-generate-preview",
          "veo-3.1-generate",
          "veo-3.1-fast-generate-preview",
          "veo-3.1-fast-generate"
        ]
      },
      "request": {
        "method": "SDK",
        "endpoint": "client.models.generate_videos",
        "sdk_call": {
          "args": [],
          "kwargs": {
            "model": "{{model}}",
            "prompt": "{{prompt}}",
            "image": "{{image}}",
            "config": {
              "$call": "types.GenerateVideosConfig",
              "$args": [],
              "$kwargs": {
                "aspectRatio": "{{config_aspectRatio}}",
                "resolution": "{{config_resolution}}",
                "durationSeconds": "{{config_durationSeconds}}",
                "personGeneration": "{{config_personGeneration}}",
                "reference_images": "{{reference_images}}",
                "last_frame": "{{last_image}}"
              }
            }
          }
        }
      }
    }
  }
}
