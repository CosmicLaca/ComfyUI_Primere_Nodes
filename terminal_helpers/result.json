{
  "Gemini": {
    "Nanobanana": {
      "provider": "Gemini",
      "service": "Nanobanana",
      "possible_parameters": {
        "aspect_ratio": ["1:1", "2:3", "3:2", "3:4", "4:3", "4:5", "5:4", "9:16", "16:9", "21:9"],
        "resolution": [
          "1K",
          "2K",
          "4K"
        ],
        "model": [
          "gemini-3-pro-image-preview",
          "gemini-2.5-flash-image"
        ]
      },
      "request": {
        "method": "SDK",
        "endpoint": "client.models.generate_content",
        "sdk_call": {
          "args": [],
          "kwargs": {
            "model": "{{model}}",
            "contents": [
              "{{prompt}}",
              "{{reference_images}}"
            ],
            "config": {
              "$call": "types.GenerateContentConfig",
              "$args": [],
              "$kwargs": {
                "response_modalities": [
                  "{{config_response_modalities_0}}"
                ],
                "image_config": {
                  "$call": "types.ImageConfig",
                  "$args": [],
                  "$kwargs": {
                    "aspect_ratio": "{{config_image_config_aspect_ratio}}",
                    "image_size": "{{config_image_config_image_size}}"
                  }
                }
              }
            }
          }
        }
      }
    },
    "VEO": {
      "provider": "Gemini",
      "service": "VEO",
      "possible_parameters": {
        "aspectRatio": ["9:16", "16:9"],
        "durationSeconds": [
          "4",
          "6",
          "8"
        ],
        "personGeneration": [
          "allow_all",
          "allow_adult",
          "dont_allow"
        ],
        "resolution": [
          "720p",
          "1080p",
          "4K"
        ],
        "model": [
          "veo-3.0-generate",
          "veo-3.0-fast-generate",
          "veo-3.1-generate-preview",
          "veo-3.1-generate",
          "veo-3.1-fast-generate-preview",
          "veo-3.1-fast-generate"
        ]
      },
      "request": {
        "method": "SDK",
        "endpoint": "client.models.generate_videos",
        "sdk_call": {
          "args": [],
          "kwargs": {
            "model": "{{model}}",
            "prompt": "{{prompt}}",
            "image": "{{first_image}}",
            "config": {
              "$call": "types.GenerateVideosConfig",
              "$args": [],
              "$kwargs": {
                "aspectRatio": "{{config_aspectRatio}}",
                "resolution": "{{config_resolution}}",
                "durationSeconds": "{{config_durationSeconds}}",
                "personGeneration": "{{config_personGeneration}}",
                "reference_images": "{{reference_images}}",
                "last_frame": "{{last_image}}"
              }
            }
          }
        }
      }
    },
    "Imagen": {
      "provider": "Gemini",
      "service": "Imagen",
      "possible_parameters": {
        "aspect_ratio": ["1:1", "2:3", "3:2", "3:4", "4:3", "4:5", "5:4", "9:16", "16:9", "21:9"],
        "resolution": [
          "1K",
          "2K"
        ],
        "model": [
          "imagen-4.0-generate-001",
          "imagen-4.0-ultra-generate-001",
          "imagen-4.0-fast-generate-001"
        ]
      },
      "request": {
        "method": "SDK",
        "endpoint": "client.models.generate_images",
        "sdk_call": {
          "args": [],
          "kwargs": {
            "model": "{{model}}",
            "prompt": "{{prompt}}",
            "config": {
              "$call": "types.GenerateImagesConfig",
              "$args": [],
              "$kwargs": {
                "number_of_images": "{{config_number_of_images}}",
                "aspect_ratio": "{{config_aspect_ratio}}",
                "image_size": "{{config_image_size}}"
              }
            }
          }
        }
      }
    }
  },
  "FAL": {
    "Seedream_V5_T2I": {
      "provider": "FAL",
      "service": "Seedream_V5_T2I",
      "possible_parameters": {
        "arguments_enable_safety_checker": [
          false,
          true
        ],
        "with_logs": [
          false,
          true
        ],
        "enhance_prompt_mode": [
          "fast",
          "standard"
        ]
      },
      "request": {
        "method": "SDK",
        "endpoint": "client.submit",
        "sdk_call": {
          "args": [
            "{{arg0}}"
          ],
          "kwargs": {
            "arguments": {
              "prompt": "{{prompt}}",
              "image_size": {
                "width": "{{arguments_image_size_width}}",
                "height": "{{arguments_image_size_height}}"
              },
              "num_images": "{{batch}}",
              "max_images": "{{arguments_max_images}}",
              "enable_safety_checker": "{{arguments_enable_safety_checker}}",
              "enhance_prompt_mode": "{{arguments_enhance_prompt_mode}}",
              "seed": "{{seed}}"
            },
            "with_logs": "{{with_logs}}"
          }
        }
      }
    },
    "Seedream_V5_I2I": {
      "provider": "FAL",
      "service": "Seedream_V5_I2I",
      "possible_parameters": {
        "arguments_enable_safety_checker": [
          false,
          true
        ],
        "with_logs": [
          false,
          true
        ],
        "enhance_prompt_mode": [
          "fast",
          "standard"
        ]
      },
      "request": {
        "method": "SDK",
        "endpoint": "lient.submit",
        "sdk_call": {
          "args": [
            "{{arg0}}"
          ],
          "kwargs": {
            "arguments": {
              "prompt": "{{prompt}}",
              "img_urls": [
                "{{images}}"
              ],
              "image_size": {
                "width": "{{arguments_image_size_width}}",
                "height": "{{arguments_image_size_height}}"
              },
              "num_images": "{{batch}}",
              "max_images": "{{arguments_max_images}}",
              "enable_safety_checker": "{{arguments_enable_safety_checker}}",
              "enhance_prompt_mode": "{{arguments_enhance_prompt_mode}}",
              "seed": "{{seed}}"
            },
            "with_logs": "{{with_logs}}"
          }
        }
      }
    }
  }
}