{
  "BlackForest": {
    "FluxFillPro": {
      "provider": "BlackForest",
      "service": "FluxFillPro",
      "possible_parameters": {
        "blackforest_guidance": "FLOAT",
        "blackforest_steps": "INT",
        "safety_tolerance": [1, 2, 3, 4, 5, 6]
      },
      "request": {
        "method": "SDK",
        "endpoint": "requests.post",
        "sdk_call": {
          "args": [
            "{{arg0}}"
          ],
          "kwargs": {
            "payload": {
              "output_format": "png",
              "image": "{{reference_images}}",
              "mask": "{{mask_images}}",
              "safety_tolerance": "{{safety_tolerance}}",
              "prompt": "{{prompt}}",
              "seed": "{{seed}}",
              "aspect_ratio": "{{aspect_ratio}}",
              "prompt_upsampling": false,
              "guidance": "{{guidance}}",
              "steps": "{{blackforest_steps}}"
            }
          }
        }
      }
    }
  }
}
